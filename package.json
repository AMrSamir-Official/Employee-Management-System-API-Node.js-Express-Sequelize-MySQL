{
  "name": "employee-management-api",
  "version": "1.0.0",
  "description": "Employee Management System",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "db:create": "npx sequelize-cli db:create",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all",
    "db:setup": "npm run db:create && npm run db:migrate && npm run db:seed",
    "db:reset": "npm run db:seed:undo && npm run db:migrate:undo && npm run db:migrate && npm run db:seed"
  },
  "keywords": [
    "nodejs",
    "express",
    "sequelize",
    "mysql",
    "api"
  ],
  "author": "Amr Samir",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "sequelize": "^6.35.2",
    "mysql2": "^3.6.5",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "cors": "^2.8.5",
    "joi": "^17.11.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "csv-writer": "^1.6.0",
    "pdfkit": "^0.13.0",
    "ejs": "^3.1.9"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2"
  }
}
